<% provide(:title, 'Sign up') %>
<h1>New User</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Create account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

<%= link_to 'Back', users_path %>
